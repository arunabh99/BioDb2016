   "source": [
    "def dna_to_rna(filein):\n",
    "    for line in filein:\n",
    "        line = line.strip()\n",
    "        return line.replace('T', 'U')\n",
    "\n",
    "filename = input('Enter the filename:')\n",
    "filein = open(filename)\n",
    "print(dna_to_rna(filein))"
   ]
